import java.util.ArrayList;

class Entity{
    int initX;
    int initY;
    int height;
    int width;

    Entity(){
        initX = 0;
        initY = 0;
        height = 100;
        width = 100;
    }

    Entity(int inputInitX, int inputInitY, int inputHeight, int inputWidth){
        initX = inputInitX;
        initY = inputInitY;
        height = inputHeight;
        width = inputWidth;
    }

    
}
